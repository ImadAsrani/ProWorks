<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>


<BorderPane fx:id="main" prefHeight="500.0" prefWidth="800.0" stylesheets="@../../../CSS/Index.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.workmanager.workmanager.Controller">
   <top>
      <HBox alignment="TOP_RIGHT" prefHeight="38.0" prefWidth="115.0" spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="minimize" mnemonicParsing="false" onAction="#windowButtons" prefHeight="25.0" prefWidth="30.0" styleClass="botonesventana" />
            <Button fx:id="maximize" mnemonicParsing="false" onAction="#windowButtons" prefHeight="25.0" prefWidth="30.0" styleClass="botonesventana" />
            <Button fx:id="close" mnemonicParsing="false" onAction="#windowButtons" prefHeight="25.0" prefWidth="30.0" styleClass="botonesventana" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </top>
</BorderPane>
